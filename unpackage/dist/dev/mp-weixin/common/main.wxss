.tal{text-align: left;}
/* 相对定位|绝对定位|固定定位 */
.fixed{position: fixed;z-index: 1;}
.pos-r{position: relative;}
.pos-f{position: fixed;z-index: 1;}
.pos-a{position: absolute;}
.pos-t-c { top: 50%; -webkit-transform: translateY(-50%); transform: translateY(-50%);
}
.pos-l-c { left: 50%; -webkit-transform: translateX(-50%); transform: translateX(-50%);
}
.pos-tl-c { top: 50%; left: 50%; -webkit-transform: translate(-50%, -50%); transform: translate(-50%, -50%);
}
.pos-top {top: 0}
